    <AssemblyName>System.ServiceModel.Primitives</AssemblyName>
    <RootNamespace>System.ServiceModel</RootNamespace>
  </PropertyGroup>

  <PropertyGroup Condition="'$(TargetGroup)' == 'uap'">
