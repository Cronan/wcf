    <NoWarn>$(NoWarn);xUnit1013;xUnit2010;xUnit2006;xUnit1014;xUnit1003;xUnit2000;xUnit2009;xUnit1006</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <!-- [todo:arcade] Is it ok to directly reference the implementation project and not the ref assemblies? -->
